    "version": "0.2.0",
    "configurations": [
    {
        "name": "Flutter: Attach to Device",
        "type": "dart",
        "request": "attach"
    },
        {
            "name": "Dart",
            "type": "dart",
            "request": "launch",
            "program": "bin/main.dart"
        },
    {
        "name": "Attach to Chrome",
        "port": 9222,
        "request": "attach",
        "type": "chrome",
        "webRoot": "${workspaceFolder}"
    },
        {
            "name": "pikachu1",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "pikachu1 (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "pikachu1 (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}